{
    "name": "norvica/pathfinder",
    "description": "Fast PHP router.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "router",
        "trie"
    ],
    "autoload": {
        "psr-4": {
            "Norvica\\Pathfinder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Norvica\\Pathfinder\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Serge Kvashnin",
            "email": "75180587+serge-kvashnin@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "psr/http-message": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "phpbench/phpbench": "^1.2"
    },
    "suggest": {
        "guzzlehttp/psr7": "Contains a full PSR-7 message implementation."
    }
}
